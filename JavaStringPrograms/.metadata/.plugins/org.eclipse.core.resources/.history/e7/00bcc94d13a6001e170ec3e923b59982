package com.java.string;

public class ReverseString {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s="ayyappa";
		
		char[] charArray=s.toCharArray();
		
		//1-Approach
		int length=s.length();
		for (int i = length-1; i>=0; i--) {
			System.out.print(charArray[i]);
		}
	}

}
